            },
            "final_gravity_max": {
                "type": "number",
                "default": 0.0,
                "title": "The final_gravity_max Schema",
                "examples": [
                    1.012
                ]
            },
            "abv_min": {
                "type": "number",
                "default": 0.0,
                "title": "The abv_min Schema",
                "examples": [
                    4.2
                ]
            },
            "abv_max": {
                "type": "number",
                "default": 0.0,
                "title": "The abv_max Schema",
                "examples": [
                    5.6
                ]
            },
            "ibu_min": {
                "type": "integer",
                "default": 0,
                "title": "The ibu_min Schema",
                "examples": [
                    15
                ]
            },
            "ibu_max": {
                "type": "integer",
                "default": 0,
                "title": "The ibu_max Schema",
                "examples": [
                    20
                ]
            },
            "color_srm_min": {
                "type": "number",
                "default": 0.0,
                "title": "The color_srm_min Schema",
                "examples": [
                    2.5
                ]
            },
            "color_srm_max": {
                "type": "number",
                "default": 0.0,
                "title": "The color_srm_max Schema",
                "examples": [
                    5.0
                ]
            },
            "description": {
                "type": "string",
                "default": "",
                "title": "The description Schema",
                "examples": [
                    "A clean, well-attenuated, flavorful American “lawnmower” beer. Easily drinkable and refreshing, with more character than typical American lagers."
                ]
            }
        },
        "examples": [{
            "name": "Cream Ale",
            "original_gravity_min": 1.042,
            "original_gravity_max": 1.055,
            "final_gravity_min": 1.006,
            "final_gravity_max": 1.012,
            "abv_min": 4.2,
            "abv_max": 5.6,
