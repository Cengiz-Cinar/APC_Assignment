{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "array",
    "default": [],
    "title": "Root Schema",
    "items": {
        "type": "object",
        "default": {},
        "title": "A Schema",
        "required": [
            "name",
            "original_gravity_min",
            "original_gravity_max",
            "final_gravity_min",
            "final_gravity_max",
            "abv_min",
            "abv_max",
            "ibu_min",
            "ibu_max",
            "color_srm_min",
            "color_srm_max",
            "description"
        ],
        "properties": {
            "name": {
                "type": "string",
                "default": "",
                "title": "The name Schema",
                "examples": [
                    "Cream Ale"
                ]
            },
            "original_gravity_min": {
                "type": "number",
                "default": 0.0,
                "title": "The original_gravity_min Schema",
